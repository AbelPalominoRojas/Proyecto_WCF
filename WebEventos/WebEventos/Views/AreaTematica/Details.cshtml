@model WebEventos.SRefAreaTematica.AreaTematica

@{
ViewBag.Title = "Details";
}

<div class="row">
    <div class="col-12 col-lg-10 col-xl-8">
        <div class="ibox">
            <div class="ibox-head">
                <div class="ibox-title">
                    Detalle Area Tematica
                </div>
            </div>
            <div class="ibox-body">
                <div class="form-group row">
                    <label class="col-sm-3 col-form-label">Codigo</label>
                    <div class="col-sm-9">
                        : @Html.DisplayFor(model => model.CodAreaTematica)
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-3 col-form-label">Area Tematica</label>
                    <div class="col-sm-9">
                        : @Html.DisplayFor(model => model.NombreAreaTematica)
                    </div>
                </div>

                <hr />
                <div class="d-flex justify-content-between">
                    @Html.ActionLink(" ", "Index",null,new { @class = "btn btn-info fa fa-reply" })
                    @Html.ActionLink(" Editar", "Edit", new { id = Model.CodAreaTematica }, new { @class = "btn btn-primary fa fa-pencil" })
                </div>
            </div>
        </div>
    </div>
</div>